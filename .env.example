# Porta do servidor Express
PORT=3000

# Troque por uma string longa e segura em produção
JWT_SECRET=troque-esta-chave

# Se quiser usar OpenCage, altere para "opencage" e preencha a chave abaixo
GEOCODER_PROVIDER=google

# Credenciais dos geocoders (obrigatório fornecer ao menos uma)
GOOGLE_MAPS_API_KEY=coloque-sua-chave-google-aqui
OPENCAGE_API_KEY=

# Parâmetros de geocodificação (opcionais)
GEOCODING_DEFAULT_CITY=Manaus
GEOCODING_DEFAULT_STATE=Amazonas
GEOCODING_DEFAULT_COUNTRY=Brasil
GEOCODING_COUNTRY_CODE=br
GEOCODING_CACHE_TTL_MS=86400000
GEOCODING_MIN_CONFIDENCE=5
GOOGLE_ALLOW_PARTIAL=1
